{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/catalog/facebook.csv",
        "function": "facebookCatalogFeed"
      },
      {
        "source": "/catalog/facebook",
        "function": "facebookCatalogFeed"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

